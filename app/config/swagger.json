            "schema": {
              "$ref": "#/definitions/ApiResponseFail"
            }
          }
        }
      }
    },
    "/upload": {
      "post": {
        "tags": [
          "upload"
        ],
        "summary": "upload document",
        "description": "upload document",
        "operationId": "uploadDocId",
        "consumes": [
          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "formData",
            "name": "document",
            "required" : true,
            "schema": {
              "type": "file"
            },
            "description": "file to upload"
          },
          {
            "in": "query",
            "name": "dbName",
            "required" : true,
            "schema": {
              "type": "string"
            },
            "description": "database name"
          },
          {
            "in": "query",
            "name": "collectionName",
            "required" : true,
            "schema": {
              "type": "string"
            },
            "description": "collection name"
          },
          {
            "in": "query",
            "name": "appName",
            "required" : true,
            "schema": {
              "type": "string"
            },
            "description": "application name"
          },
          {
            "in": "query",
            "name": "filePath",
            "required" : true,
            "schema": {
              "type": "string"
            },
            "description": "file path"
          },
          {
            "in": "query",
            "name": "isEncrypted",
            "required" : true,
            "schema": {
              "type": "boolean"
            },
            "description": "file path encryption flag"
          }
        ],
        "responses": {
          "200": {
            "description": "response success",
            "schema": {
            "example" : {
              "status": true,
              "statusCode": "success",
              "statusMessage": "Success",
              "responseCode": 200,
              "response": {
